  <DTS:Variables>
    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{A21F94B5-C15C-458C-ABF7-C84D9ED4F4F8}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;WITH XCOM_List AS &#xA; (&#xA;SELECT DISTINCT &#xA;&#x9;[Brand] = XS.Brand,&#xA;&#x9;[Title] = lower(ltrim(rtrim(XS.title))), &#xA;&#x9;[FirstName] = lower(ltrim(rtrim(XS.firstname))), &#xA;&#x9;[LastName] = lower(ltrim(rtrim(XS.lastname))), &#xA;&#x9;[Email] =[Data_Control].[hlpcode].[fnGetCleanEmail_Extended](XS.Email),&#xA;&#x9;[DOB] = XS.dob, &#xA;&#x9;[Phone] = XS.phone, &#xA;&#x9;[Mobile] = XS.mobile, &#xA;&#x9;[Postcode] = XS.postcode, &#xA;&#x9;[State] = UPPER(XS.state),       &#xA;&#x9;[SA1Code] = Lookup_SA1_Codes.[sa1],&#xA;&#x9;[CommunityCode] = Lookup_SA1_Codes.[Community Code],&#xA;&#x9;[CommunityName] = Lookup_SA1_Codes.[Community Name],&#xA;&#x9;[PersonaCode] = Lookup_SA1_Codes.[Persona Code],&#xA;&#x9;[PersonaName] = Lookup_SA1_Codes.[Persona Name],&#xA;&#x9;[RowNumber] = row_number() over(partition by [Data_Control].[hlpcode].[fnGetCleanEmail_Extended](XS.Email) &#xA;&#x9;order by [Data_Control].[hlpcode].[fnGetCleanEmail_Extended](lower(ltrim(rtrim(XS.Email)))))&#xA;FROM &#xA;[DNA_Mkt_SourceData].[rawdata].[XCOMUnSubscribes] AS XS&#xA;INNER JOIN [DNA_Mkt_Main].[persdata].[BioEmailAttributes] AS EmailAddr ON &#xA;&#x9;lower(ltrim(rtrim(EmailAddr.Email))) = lower(ltrim(rtrim(XS.Email))) AND EmailAddr.StatusCode = 'A'&#xA;INNER JOIN [DNA_Mkt_SourceData].[rawdata].[BioAddrAttributes_SV3_Staging] AS BAA_Stage ON EmailAddr.DNAID = BAA_Stage.DNAID&#xA;INNER JOIN [DNA_Mkt_Main].[persdata].[BioAddrAttributes] Addr ON BAA_Stage.AddrAttribID = Addr.AddrAttribID AND Addr.[StatusCode] = 'A'&#xA;INNER JOIN [DNA_Mkt_SourceData].[rawdata].[RDM_SA1_Persona] AS Lookup_SA1_Codes ON Lookup_SA1_Codes.[sa1] = BAA_Stage.[SA1Code]&#xA;WHERE &#xA;[Data_Control].[hlpcode].[fnGetCleanEmail_Extended](XS.Email) IS NOT NULL) &#xA;SELECT &#xA;&#x9;* &#xA;FROM &#xA;&#x9;XCOM_List&#xA;WHERE &#xA;&#x9;[RowNumber] &gt; 1 &#xA;ORDER BY Email&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="SourceSQL">
      <DTS:VariableValue
        DTS:DataType="8">WITH XCOM_List AS 
 (
SELECT DISTINCT 
	[Brand] = XS.Brand,
	[Title] = lower(ltrim(rtrim(XS.title))), 
	[FirstName] = lower(ltrim(rtrim(XS.firstname))), 
	[LastName] = lower(ltrim(rtrim(XS.lastname))), 
	[Email] =[Data_Control].[hlpcode].[fnGetCleanEmail_Extended](XS.Email),
	[DOB] = XS.dob, 
	[Phone] = XS.phone, 
	[Mobile] = XS.mobile, 
	[Postcode] = XS.postcode, 
	[State] = UPPER(XS.state),       
	[SA1Code] = Lookup_SA1_Codes.[sa1],
	[CommunityCode] = Lookup_SA1_Codes.[Community Code],
	[CommunityName] = Lookup_SA1_Codes.[Community Name],
	[PersonaCode] = Lookup_SA1_Codes.[Persona Code],
	[PersonaName] = Lookup_SA1_Codes.[Persona Name],
	[RowNumber] = row_number() over(partition by [Data_Control].[hlpcode].[fnGetCleanEmail_Extended](XS.Email) 
	order by [Data_Control].[hlpcode].[fnGetCleanEmail_Extended](lower(ltrim(rtrim(XS.Email)))))
FROM 
[DNA_Mkt_SourceData].[rawdata].[XCOMUnSubscribes] AS XS
INNER JOIN [DNA_Mkt_Main].[persdata].[BioEmailAttributes] AS EmailAddr ON 
	lower(ltrim(rtrim(EmailAddr.Email))) = lower(ltrim(rtrim(XS.Email))) AND EmailAddr.StatusCode = 'A'
INNER JOIN [DNA_Mkt_SourceData].[rawdata].[BioAddrAttributes_SV3_Staging] AS BAA_Stage ON EmailAddr.DNAID = BAA_Stage.DNAID
INNER JOIN [DNA_Mkt_Main].[persdata].[BioAddrAttributes] Addr ON BAA_Stage.AddrAttribID = Addr.AddrAttribID AND Addr.[StatusCode] = 'A'
INNER JOIN [DNA_Mkt_SourceData].[rawdata].[RDM_SA1_Persona] AS Lookup_SA1_Codes ON Lookup_SA1_Codes.[sa1] = BAA_Stage.[SA1Code]
WHERE 
[Data_Control].[hlpcode].[fnGetCleanEmail_Extended](XS.Email) IS NOT NULL) 
SELECT 
	* 
FROM 
	XCOM_List
WHERE 
	[RowNumber] &gt; 1 
ORDER BY Email</DTS:VariableValue>
    </DTS:Variable>
  </DTS:Variables>

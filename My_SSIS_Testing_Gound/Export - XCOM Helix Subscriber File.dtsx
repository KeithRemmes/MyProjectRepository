/// <summary>
/// This script will remove characters that are not defined in the toreplace variable from the data in the appropriate column for an input row.
/// In this case, any extended/non-keyboard characters will be removed from the string.
/// </summary>
[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{
    public override void PreExecute()
    {
        base.PreExecute();
    }

    public override void PostExecute()
    {
        base.PostExecute();
    }
    static string toreplace = @"[^^a-zA-Z0-9 `!@#$%^&*()_+\-={}'?./,]";
    static string replacewith = "_";
    static Regex reg = new Regex(toreplace);

    static List<int> stringColumns = null;
    public override void ProcessInput(int InputID, PipelineBuffer Buffer)
    {
        if (stringColumns == null)
        {
            List<int> sc = new List<int>();
            for (int columnIndex = 0;
      columnIndex < Buffer.ColumnCount;
      columnIndex++)
            {
                BufferColumn columnInfo = Buffer.GetColumnInfo(columnIndex);
                if (columnInfo.DataType == DataType.DT_WSTR || columnInfo.DataType == DataType.DT_STR)
                {
                    sc.Add(columnIndex);
                }
            }
            stringColumns = sc;
        }

        /*        bool fireAgain = true;
                ComponentMetaData.FireInformation(0, "",
                  Buffer.ColumnCount.ToString() + " columns",
                  "", 0, ref fireAgain);*/

        while (Buffer.NextRow())
        {
            foreach (int columnIndex in stringColumns)
            {
                if (!Buffer.IsNull(columnIndex))
                {
                    string columnData = Buffer.GetString(columnIndex);
                    columnData = reg.Replace(StringUtility.RemoveDiacritics(columnData, true), replacewith);
                    Buffer.SetString(columnIndex, columnData);
                }
            }
        }
        base.ProcessInput(InputID, Buffer);

    }
    /*
               if(string.IsNullOrEmpty(Row.CreatedBy)) {
                    Row.CreatedBy = reg.Replace(StringUtility.RemoveDiacritics(Row.CreatedBy,true), replacewith);
               }

               if(string.IsNullOrEmpty(Row.ModifiedBy)) {
                    Row.ModifiedBy = reg.Replace(StringUtility.RemoveDiacritics(Row.ModifiedBy,true), replacewith);
               }

               if(string.IsNullOrEmpty(Row.TravelType)) {
                    Row.TravelType = reg.Replace(StringUtility.RemoveDiacritics(Row.TravelType,true), replacewith);
               }
        
               if(string.IsNullOrEmpty(Row.Type)) {
                    Row.Type = reg.Replace(StringUtility.RemoveDiacritics(Row.Type,true), replacewith);
               }
        
            }
    */

}


// from http://stackoverflow.com/questions/3769457/how-can-i-remove-accents-on-a-string
public class StringUtility
{
    public static IEnumerable<char> RemoveDiacriticsEnum(string src, bool compatNorm, Func<char, char> customFolding)
    {
        foreach (char c in src.Normalize(compatNorm ? NormalizationForm.FormKD : NormalizationForm.FormD))
            switch (CharUnicodeInfo.GetUnicodeCategory(c))
            {
                case UnicodeCategory.NonSpacingMark:
                case UnicodeCategory.SpacingCombiningMark:
                case UnicodeCategory.EnclosingMark:
                    //do nothing
                    break;
                default:
                    yield return customFolding(c);
                    break;
            }
    }
    public static IEnumerable<char> RemoveDiacriticsEnum(string src, bool compatNorm)
    {
        return RemoveDiacritics(src, compatNorm, c => c);
    }
    public static string RemoveDiacritics(string src, bool compatNorm, Func<char, char> customFolding)
    {
        StringBuilder sb = new StringBuilder();
        foreach (char c in RemoveDiacriticsEnum(src, compatNorm, customFolding))
            sb.Append(c);
        return sb.ToString();
    }
    public static string RemoveDiacritics(string src, bool compatNorm)
    {
        return RemoveDiacritics(src, compatNorm, c => c);
    }

}


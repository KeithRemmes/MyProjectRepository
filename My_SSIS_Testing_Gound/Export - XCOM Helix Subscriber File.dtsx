                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH ExportFile AS 
(
	SELECT
		SA1Data.[Brand]
		,SA1Data.[Title]
		,SA1Data.[FirstName]
		,SA1Data.[LastName]
		,SA1Data.[Email]
		,SA1Data.[DOB]
		,SA1Data.[Phone]
		,SA1Data.[Mobile]
		,SA1Data.[Postcode]
		,SA1Data.[State]
		,SA1Data.[SA1Code]
		,SA1Data.[CommunityCode]
		,SA1Data.[CommunityName]
		,SA1Data.[PersonaCode]
		,SA1Data.[PersonaName]
		,SA1Data.[DNAID]
		,SA1Data.[SA1CodeModifiedDate]
	, [RowNumber1] = row_number() over(partition BY SA1Data.Brand, SA1Data.FirstName, SA1Data.LastName, SA1Data.Email 
			ORDER by  SA1Data.Brand, SA1Data.FirstName, SA1Data.LastName, SA1Data.Email, SA1Data.SA1CodeModifiedDate, SA1Data.DNAID DESC) 
	FROM [Data_Export_Jobs].[export].[tbl_Export_XCOM_Marketing_Data_SA1Data] SA1Data
)
SELECT * FROM ExportFile EF WHERE 
		[RowNumber1] = 1 

		AND
--try_convert(date, EF.[SA1CodeModifiedDate]) &gt;= try_convert(date, '2015-04-07')
EF.[SA1CodeModifiedDate] = (SELECT MAX(MaxDate.[SA1CodeModifiedDate])
         FROM [Data_Export_Jobs].[export].[tbl_Export_XCOM_Marketing_Data_SA1Data] MaxDate)
ORDER BY [Brand], [Email]</property>

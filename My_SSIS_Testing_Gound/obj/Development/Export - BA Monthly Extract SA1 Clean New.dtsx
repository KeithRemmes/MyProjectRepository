        {
            base.PostExecute();
         }
        static string toreplace = @"[^^a-zA-Z0-9 `!@#$%^&*()_+\-={}'?./]"; 
        static string replacewith = "_";
        static Regex reg = new Regex(toreplace);

        static List<int> stringColumns = null;
        public override void ProcessInput(int InputID, PipelineBuffer Buffer)
        {
            if (stringColumns == null)
            {
                List<int> sc = new List<int>();
                for (int columnIndex = 0;
          columnIndex < Buffer.ColumnCount;
          columnIndex++)
                {
                    BufferColumn columnInfo = Buffer.GetColumnInfo(columnIndex);
                    if (columnInfo.DataType == DataType.DT_WSTR || columnInfo.DataType == DataType.DT_STR)
                    {
                        sc.Add(columnIndex);
                    }
                }
                stringColumns = sc;
            }

    /*        bool fireAgain = true;
            ComponentMetaData.FireInformation(0, "",
              Buffer.ColumnCount.ToString() + " columns",
              "", 0, ref fireAgain);*/

            while (Buffer.NextRow())
            {
                foreach (int columnIndex in stringColumns)
                {
                    if (!Buffer.IsNull(columnIndex))
                    {
                        string columnData = Buffer.GetString(columnIndex);
                        columnData = reg.Replace(StringUtility.RemoveDiacritics(columnData,true), replacewith);
                        Buffer.SetString(columnIndex, columnData);
                    }
                }
            }
            base.ProcessInput(InputID, Buffer);
            

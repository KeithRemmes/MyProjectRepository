                {
                    BufferColumn columnInfo = Buffer.GetColumnInfo(columnIndex);
                    if (columnInfo.DataType == DataType.DT_WSTR || columnInfo.DataType == DataType.DT_STR)
                    {
                        sc.Add(columnIndex);
                    }
                }
                stringColumns = sc;
            }

    /*        bool fireAgain = true;
